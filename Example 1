class Student_Result:
    def __init__(self,name,marks):
        self.name = name
        self.marks = marks
    def check_pass_fail(self):
        if self.marks >= 40:
            return True
        else:
            return False
p1 = Student_Result("Mohsin",95)
print(p1.name)
print(p1.marks)
passed1 = p1.check_pass_fail()
print(passed1)

p2 = Student_Result("Ali",35)
print(p2.name)
print(p2.marks)
passed2 = p2.check_pass_fail()
print(passed2)
